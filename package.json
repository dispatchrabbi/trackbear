{
  "name": "trackbear",
  "private": true,
  "version": "1.3.4",
  "type": "module",
  "engines": {
    "node": ">=22.11.0 <23.0.0"
  },
  "imports": {
    "src/*": "./src/*",
    "server/*": "./server/*",
    "generated/*": "./generated/*"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "node --import tsx ./prisma/seed/seed.ts"
  },
  "scripts": {
    "start": "node --run start:dev",
    "start:dev": "NODE_ENV=development node --import tsx --import ./instrumentation.ts ./main.ts",
    "start:prod": "NODE_ENV=production node --import tsx --import ./instrumentation.ts ./main.ts",
    "help:dev": "vitepress dev help-docs",
    "help:build": "vitepress build help-docs",
    "help:preview": "vitepress preview help-docs",
    "help:zip": "./scripts/zip-trackbear-help.sh",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:webui": "vitest --ui",
    "test:integration": "vitest run --config ./vitest.integration.config.js",
    "coverage": "vitest run --coverage",
    "lint": "npm run type-check && npm run eslint",
    "eslint": "npx eslint --fix --prune-suppressions .",
    "type-check": "npx vue-tsc --noEmit",
    "prebuild": "node --run compile && node --run test",
    "build": "node --run build:prod",
    "build:prod": "docker build -t dispatchrabbi/trackbear:$(npm pkg get version --workspaces=false | tr -d \\\") -t dispatchrabbi/trackbear:latest --target prod .",
    "build:prod:progress": "docker build --progress=plain -t dispatchrabbi/trackbear:$(npm pkg get version --workspaces=false | tr -d \\\") -t dispatchrabbi/trackbear:latest --target prod .",
    "compile": "node --run compile:db && node --run compile:client && node --run compile:api && node --run compile:help",
    "compile:db": "npx prisma generate --no-hints",
    "compile:client": "npx vue-tsc && npx vite build --emptyOutDir",
    "compile:api": "npx tsp compile .",
    "compile:help": "node --run help:build",
    "api": "node --run compile:api",
    "migrate:dev": "npx prisma migrate dev && npx prisma generate",
    "migrate:prod": "npx prisma migrate deploy",
    "preversion": "node --run build",
    "version": "VERSION=$(npm pkg get version --workspaces=false | tr -d \\\"); git add .; git commit -m \"v${VERSION}\"; git tag -a \"v${VERSION}\" -m \"v${VERSION}\""
  },
  "dependencies": {
    "@observablehq/plot": "^0.6.15",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.0",
    "@opentelemetry/exporter-logs-otlp-http": "^0.203.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.203.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.203.0",
    "@opentelemetry/instrumentation": "^0.203.0",
    "@opentelemetry/instrumentation-express": "^0.52.0",
    "@opentelemetry/instrumentation-http": "^0.203.0",
    "@opentelemetry/instrumentation-winston": "^0.48.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-metrics": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@opentelemetry/sdk-trace-node": "^2.0.1",
    "@opentelemetry/semantic-conventions": "^1.36.0",
    "@opentelemetry/winston-transport": "^0.14.0",
    "@prisma/adapter-pg": "^6.11.1",
    "@prisma/client": "^6.2.0",
    "@prisma/instrumentation": "^6.12.0",
    "@quixo3/prisma-session-store": "^3.1.13",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vueuse/core": "^13.0.0",
    "autoprefixer": "^10.4.16",
    "b36": "^1.0.0",
    "better-queue": "^3.8.12",
    "better-queue-sql": "^1.0.6",
    "body-parser": "^1.20.2",
    "color": "^4.2.3",
    "compression": "^1.7.4",
    "core-js": "^3.38.1",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "d3": "^7.9.0",
    "date-fns": "^4.1.0",
    "deep-eql": "^5.0.2",
    "dotenv": "^16.3.1",
    "express": "^4.21.1",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.1",
    "helmet": "^7.1.0",
    "install": "^0.13.0",
    "libcurl.js": "^0.7.0",
    "mailersend": "^2.2.0",
    "markdown-it": "^14.0.0",
    "material-design-icons-iconfont": "^6.7.0",
    "mitt": "^3.0.1",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "natural-orderby": "^4.0.0",
    "npm": "^11.3.0",
    "pg": "^8.11.3",
    "pinia": "^3.0.3",
    "plausible-tracker": "^0.3.9",
    "postcss": "^8.4.32",
    "primeicons": "^7.0.0",
    "primevue": "^3.53.0",
    "sharp": "^0.34.1",
    "tailwindcss": "^3.4.13",
    "tsx": "^4.11.0",
    "vite": "^7.0.0",
    "vitepress-openapi": "^0.1.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue-tsc": "^3.0.1",
    "winston": "^3.17.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@types/better-queue": "^3.8.6",
    "@types/color": "^3.0.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.19",
    "@types/d3": "^7.4.3",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/markdown-it": "^13.0.7",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@typespec/compiler": "^1.1.0",
    "@typespec/http": "^1.1.0",
    "@typespec/openapi": "^1.1.0",
    "@typespec/openapi3": "^1.1.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vitest/ui": "^3.2.4",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.3.0",
    "prisma": "^6.11.1",
    "rollup-plugin-visualizer": "^5.11.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.26.0",
    "uuid": "^11.1.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitepress": "^1.6.3",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^2.0.2"
  }
}
