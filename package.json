{
  "name": "writing-tracker",
  "private": true,
  "version": "0.4.1",
  "type": "module",
  "imports": {
    "src/*": "./src/*",
    "server/*": "./server/*"
  },
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "NODE_ENV=development node --import ./ts-node-loader.js ./main.ts",
    "start:prod": "NODE_ENV=production node --import ./ts-node-loader.js ./main.ts",
    "lint": "npx eslint --ext .js,.ts,.vue .",
    "build": "npx prisma generate && npm run build:client",
    "build:client": "npx vue-tsc && npx vite build --emptyOutDir",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:prod": "npx prisma migrate deploy && npx prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@quixo3/prisma-session-store": "^3.1.13",
    "better-queue": "^3.8.12",
    "better-queue-sqlite": "^1.0.7",
    "body-parser": "^1.20.2",
    "chart.js": "^4.4.1",
    "compression": "^1.7.4",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "mailersend": "^2.2.0",
    "morgan": "^1.10.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.8",
    "vue-chartjs": "^5.2.0",
    "vue-router": "^4.2.5",
    "vuestic-ui": "^1.8.5",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/better-queue": "^3.8.6",
    "@types/compression": "^1.7.5",
    "@types/dotenv-safe": "^8.1.5",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/morgan": "^1.9.9",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vuestic/tailwind": "^0.1.3",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-vue": "^9.19.2",
    "material-design-icons-iconfont": "^6.7.0",
    "postcss": "^8.4.32",
    "prisma": "^5.6.0",
    "rollup-plugin-visualizer": "^5.11.0",
    "tailwindcss": "^3.3.6",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.22"
  }
}
