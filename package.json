{
  "name": "trackbear",
  "private": true,
  "version": "1.1.15",
  "type": "module",
  "engines": {
    "node": ">=22.11.0 <23.0.0"
  },
  "imports": {
    "src/*": "./src/*",
    "server/*": "./server/*"
  },
  "prisma": {
    "seed": "node --import tsx ./prisma/seed/seed.ts"
  },
  "scripts": {
    "start": "node --run start:dev",
    "start:dev": "NODE_ENV=development node --import tsx --import ./instrumentation.ts ./main.ts",
    "start:prod": "NODE_ENV=production node --import tsx --import ./instrumentation.ts ./main.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:webui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "build": "node --run build:prod",
    "build:prod": "docker build -t dispatchrabbi/trackbear:$(npm pkg get version --workspaces=false | tr -d \\\") -t dispatchrabbi/trackbear:latest --target prod .",
    "build:prod:progress": "docker build --progress=plain -t dispatchrabbi/trackbear:$(npm pkg get version --workspaces=false | tr -d \\\") -t dispatchrabbi/trackbear:latest --target prod .",
    "lint": "npm run type-check && npm run eslint",
    "eslint": "npx eslint .",
    "type-check": "npx vue-tsc --noEmit",
    "precompile": "node --run test",
    "compile": "node --run compile:db && node --run compile:client",
    "compile:db": "npx prisma generate",
    "compile:client": "npx vue-tsc && npx vite build --emptyOutDir",
    "migrate:dev": "npx prisma migrate dev && npx prisma generate",
    "migrate:prod": "npx prisma migrate deploy",
    "hooks": "husky install",
    "preversion": "node --run build",
    "version": "VERSION=$(npm pkg get version --workspaces=false | tr -d \\\"); git add .; git commit -m \"v${VERSION}\"; git tag -a \"v${VERSION}\" -m \"v${VERSION}\""
  },
  "dependencies": {
    "@observablehq/plot": "^0.6.15",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.50.0",
    "@opentelemetry/exporter-logs-otlp-http": "^0.55.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.53.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.53.0",
    "@opentelemetry/instrumentation-express": "^0.42.0",
    "@opentelemetry/instrumentation-http": "^0.53.0",
    "@opentelemetry/instrumentation-winston": "^0.42.0",
    "@opentelemetry/resources": "^1.26.0",
    "@opentelemetry/sdk-logs": "^0.55.0",
    "@opentelemetry/sdk-metrics": "^1.26.0",
    "@opentelemetry/sdk-node": "^0.53.0",
    "@opentelemetry/sdk-trace-node": "^1.26.0",
    "@opentelemetry/semantic-conventions": "^1.27.0",
    "@opentelemetry/winston-transport": "^0.8.0",
    "@prisma/client": "^6.0.1",
    "@prisma/instrumentation": "^5.20.0",
    "@quixo3/prisma-session-store": "^3.1.13",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vueuse/core": "^11.1.0",
    "autoprefixer": "^10.4.16",
    "better-queue": "^3.8.12",
    "better-queue-sql": "^1.0.6",
    "body-parser": "^1.20.2",
    "color": "^4.2.3",
    "compression": "^1.7.4",
    "core-js": "^3.38.1",
    "cron": "^3.1.6",
    "d3": "^7.9.0",
    "date-fns": "^4.1.0",
    "deep-eql": "^5.0.2",
    "dotenv": "^16.3.1",
    "express": "^4.21.1",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.1",
    "helmet": "^7.1.0",
    "mailersend": "^2.2.0",
    "markdown-it": "^14.0.0",
    "material-design-icons-iconfont": "^6.7.0",
    "mitt": "^3.0.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "natural-orderby": "^4.0.0",
    "pg": "^8.11.3",
    "pinia": "^2.1.7",
    "plausible-tracker": "^0.3.9",
    "postcss": "^8.4.32",
    "primeicons": "^7.0.0",
    "primevue": "^3.53.0",
    "tailwindcss": "^3.4.13",
    "tsx": "^4.11.0",
    "vite": "^6.0.9",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue-tsc": "^2.1.10",
    "winston": "^3.17.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/color": "^3.0.6",
    "@types/compression": "^1.7.5",
    "@types/d3": "^7.4.3",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/markdown-it": "^13.0.7",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vitest/coverage-v8": "^3.0.4",
    "@vitest/ui": "^3.0.4",
    "@vuestic/tailwind": "^0.1.3",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.32.0",
    "husky": "^8.0.3",
    "prettier": "3.3.3",
    "prisma": "^6.0.1",
    "rollup-plugin-visualizer": "^5.11.0",
    "typescript": "5.6.3",
    "typescript-eslint": "^8.11.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.4",
    "vitest-mock-extended": "^2.0.2"
  }
}
