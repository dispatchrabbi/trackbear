{
  "name": "writing-tracker",
  "private": true,
  "version": "0.1.4-alpha",
  "type": "module",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "NODE_ENV=development npx ts-node --esm ./main.ts",
    "start:prod": "NODE_ENV=production npx ts-node --esm ./main.ts",
    "start:argh": "NODE_ENV=production DATABASE_URL=\"file:../../db/trackbear.db\" npx ts-node --esm ./main.ts",
    "lint": "npx eslint --ext .js,.ts,.vue .",
    "client:dev": "cd ./client && npx vite",
    "client:build": "cd ./client && npx vue-tsc && npx vite build --emptyOutDir",
    "client:preview": "cd ./client && npx vite preview",
    "db:migrate": "npx prisma migrate dev"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@quixo3/prisma-session-store": "^3.1.13",
    "body-parser": "^1.20.2",
    "chart.js": "^4.4.1",
    "date-fns": "^2.30.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.8",
    "vue-chartjs": "^5.2.0",
    "vue-router": "^4.2.5",
    "vuestic-ui": "^1.8.5",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/dotenv-safe": "^8.1.5",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/morgan": "^1.9.9",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vuestic/tailwind": "^0.1.3",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-vue": "^9.19.2",
    "material-design-icons-iconfont": "^6.7.0",
    "postcss": "^8.4.32",
    "prisma": "^5.6.0",
    "rollup-plugin-visualizer": "^5.11.0",
    "tailwindcss": "^3.3.6",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.22"
  }
}
