{
  "name": "writing-tracker",
  "private": true,
  "version": "0.9.3",
  "type": "module",
  "imports": {
    "src/*": "./src/*",
    "server/*": "./server/*"
  },
  "prisma": {
    "seed": "node --import ./ts-node-loader.js ./prisma/seed/seed.ts"
  },
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "NODE_ENV=development node --import ./ts-node-loader.js ./main.ts",
    "start:prod": "NODE_ENV=production node --import ./ts-node-loader.js ./main.ts",
    "build": "npm run build:prod",
    "build:prod": "docker build -t dispatchrabbi/trackbear:$(npm pkg get version --workspaces=false | tr -d \\\") -t dispatchrabbi/trackbear:latest --target prod .",
    "lint": "npx eslint --ext .js,.ts,.vue .",
    "compile": "npm run compile:db && npm run compile:client",
    "compile:db": "npx prisma generate",
    "compile:client": "npx vue-tsc && npx vite build --emptyOutDir",
    "hooks": "husky install",
    "preversion": "npm run build",
    "version": "VERSION=$(npm pkg get version --workspaces=false | tr -d \\\"); git add .; git commit -m \"v${VERSION}\"; git tag -a \"v${VERSION}\" -m \"v${VERSION}\""
  },
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "@quixo3/prisma-session-store": "^3.1.13",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vueuse/core": "^10.9.0",
    "autoprefixer": "^10.4.16",
    "better-queue": "^3.8.12",
    "better-queue-sql": "^1.0.6",
    "body-parser": "^1.20.2",
    "chart.js": "^4.4.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-chart-matrix": "^2.0.1",
    "color": "^4.2.3",
    "compression": "^1.7.4",
    "cron": "^3.1.6",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "mailersend": "^2.2.0",
    "markdown-it": "^14.0.0",
    "material-design-icons-iconfont": "^6.7.0",
    "mitt": "^3.0.1",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "pinia": "^2.1.7",
    "postcss": "^8.4.32",
    "primeicons": "^7.0.0",
    "primevue": "^3.50.0",
    "prisma": "^5.11.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.1",
    "vite": "^5.0.0",
    "vue": "^3.3.8",
    "vue-chartjs": "^5.2.0",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.22",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/color": "^3.0.6",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/markdown-it": "^13.0.7",
    "@types/morgan": "^1.9.9",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vuestic/tailwind": "^0.1.3",
    "eslint": "^8.55.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-vue": "^9.19.2",
    "husky": "^8.0.3",
    "rollup-plugin-visualizer": "^5.11.0",
    "typescript": "^5.3.2"
  }
}
